{
  "name": "strapi-plugin-comments",
  "version": "2.0.10",
  "description": "Strapi - Comments plugin",
  "strapi": {
    "name": "comments",
    "displayName": "Comments",
    "description": "Powerful Strapi based comments moderation tool for you and your users",
    "kind": "plugin"
  },
  "scripts": {
    "prepare": "husky install",
    "publish:latest": "cd build && npm publish --tag latest",
    "prepublish:latest": "npm run clean && npm run build && node build/setup-package.js",
    "build": "tsc",
    "build:dev": "npm run build && cp ./package.json ./build && cd ./build && yarn",
    "clean": "rm -rf build",
    "develop": "nodemon --exec \"npm run build:dev\"",
    "test:unit": "jest --verbose --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtusLab-Open-Source/strapi-plugin-comments"
  },
  "dependencies": {
    "@strapi/plugin-graphql": "^4.1.2",
    "@strapi/utils": "^4.1.2",
    "bad-words": "^3.0.4"
  },
  "devDependencies": {
    "@types/bad-words": "^3.0.1",
    "@types/jest": "^27.4.1",
    "codecov": "^3.7.2",
    "husky": "7.0.4",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "koa": "2.13.3",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@strapi/strapi": "4.x"
  },
  "nodemonConfig": {
    "ignore": [
      "./build/**/*"
    ],
    "ext": "js,json,ts"
  },
  "author": {
    "name": "VirtusLab",
    "email": "strapi@virtuslab.com",
    "url": "https://virtuslab.com"
  },
  "maintainers": [
    {
      "name": "VirtusLab // Mateusz Ziarko",
      "email": "mziarko@virtuslab.com",
      "url": "https://virtuslab.com"
    },
    {
      "name": "VirtusLab // Maksymilian PamuÅ‚a",
      "email": "mpamula@virtuslab.com",
      "url": "https://virtuslab.com"
    }
  ],
  "engines": {
    "node": ">=14.x.x <=17.x.x",
    "npm": ">=7.x.x"
  },
  "keywords": [
    "strapi",
    "plugin",
    "comments"
  ],
  "license": "MIT"
}
